{
  "name": "fontawesome-svelte",
  "description": "@fortawesome/vue-fontawesome implementation for Svelte",
  "version": "3.0.0",
  "author": "Kirill Sedunov <kirill.sedunov@outlook.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yukipastelcat/svelte-fontawesome.git"
  },
  "homepage": "https://yukipastelcat.github.io/svelte-fontawesome",
  "keywords": [
    "fontawesome",
    "svelte",
    "typescript",
    "svelte5",
    "icons",
    "frontend"
  ],
  "type": "module",
  "module": "./dist/fontawesome-svelte.js",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "svelte": "./dist/fontawesome-svelte.js"
    },
    "./FontAwesomeIcon.svelte": {
      "types": "./dist/lib/FontAwesomeIcon.svelte.d.ts",
      "svelte": "./src/lib/FontAwesomeIcon.svelte"
    },
    "./FontAwesomeLayers.svelte": {
      "types": "./dist/lib/FontAwesomeLayers.svelte.d.ts",
      "svelte": "./src/lib/FontAwesomeLayers.svelte"
    },
    "./FontAwesomeLayersText.svelte": {
      "types": "./dist/lib/FontAwesomeLayersText.svelte.d.ts",
      "svelte": "./src/lib/FontAwesomeLayersText.svelte"
    }
  },
  "scripts": {
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "build": "vite build",
    "test": "vitest",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "lint": "eslint ."
  },
  "peerDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.3",
    "@eslint/compat": "^1.2.4",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-svelte-csf": "^5.0.0-next.21",
    "@storybook/blocks": "^8.4.7",
    "@storybook/svelte": "^8.4.7",
    "@storybook/svelte-vite": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@testing-library/svelte": "^5.2.6",
    "@tsconfig/svelte": "^5.0.4",
    "@types/node": "^22.10.5",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint": "^9.17.0",
    "eslint-plugin-svelte": "^2.46.1",
    "jest-dom": "^4.0.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.4.2",
    "storybook": "^8.4.7",
    "svelte": "^5.15.0",
    "svelte-check": "^4.1.1",
    "svelte-eslint-parser": "^0.43.0",
    "typescript": "~5.6.2",
    "vite": "^6.0.5",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^2.1.8"
  }
}
