{
	"name": "fontawesome-svelte",
	"version": "3.0.0",
	"description": "@fortawesome/vue-fontawesome implementation for Svelte",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/yukipastelcat/svelte-fontawesome.git"
	},
	"homepage": "https://yukipastelcat.github.io/svelte-fontawesome",
	"author": "Kirill Sedunov <kirill.sedunov@outlook.com>",
	"keywords": [
		"fontawesome",
		"svelte",
		"icons",
		"frontend"
	],
	"license": "MIT",
	"scripts": {
		"prettier:write": "prettier --write .",
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"storybook": "storybook dev -p 6006",
		"storybook:build": "storybook build"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"@fortawesome/fontawesome-svg-core": "^1.2.26",
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^3.2.3",
		"@eslint/compat": "^1.2.3",
		"@eslint/js": "^9.17.0",
		"@fortawesome/fontawesome-svg-core": "^6.7.2",
		"@fortawesome/free-brands-svg-icons": "^6.7.2",
		"@fortawesome/free-regular-svg-icons": "^6.7.2",
		"@fortawesome/free-solid-svg-icons": "^6.7.2",
		"@storybook/addon-essentials": "^8.4.7",
		"@storybook/addon-interactions": "^8.4.7",
		"@storybook/addon-svelte-csf": "^5.0.0-next.21",
		"@storybook/blocks": "^8.4.7",
		"@storybook/svelte": "^8.4.7",
		"@storybook/sveltekit": "^8.4.7",
		"@storybook/test": "^8.4.7",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@testing-library/svelte": "^5.2.6",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"happy-dom": "^16.5.3",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"publint": "^0.2.0",
		"storybook": "^8.4.7",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.4.11",
		"vitest": "^2.0.4"
	}
}
